{
	"_version": "1.7.0",
	"sap.app": {
		"id": "bala.comshopfloor_portal",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZSFLOGIN_SRV": {
				"uri": "/sap/opu/odata/sap/ZSFLOGIN_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "bala.comshopfloor_portal.view.login",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "bala.comshopfloor_portal.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "bala.comshopfloor_portal.view",
				"targetAggregation": "pages"
			},
			"targets": {
				"View1": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "View1",
					"viewLevel": 1,
					"targetAggregation": "pages",
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"login": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "login",
					"viewLevel": 2,
					"targetAggregation": "pages",
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"sfdashboard": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "sfdashboard",
					"viewLevel": 3,
					"targetAggregation": "pages",
					"controlId": "app",
					"controlAggregation": "pages"
				}
			},
			"routes": [{
				"name": "View1",
				"pattern": "View1",
				"titleTarget": "",
				"greedy": false,
				"targetAggregation": "pages",
				"controlId": "app",
				"target": ["View1"]
			}, {
				"name": "login",
				"pattern": "",
				"target": ["login"],
				"titleTarget": "",
				"greedy": false,
				"targetAggregation": "pages",
				"controlId": "app"
			}, {
				"name": "sfdashboard",
				"pattern": "sfdashboard",
				"target": ["sfdashboard"],
				"titleTarget": "",
				"greedy": false,
				"targetAggregation": "pages",
				"controlId": "app"
			}]
		}
	}
}